{
  "name": "quickpaste-server",
  "version": "0.0.1",
  "description": "Backend Server for Quickpaste App",
  "private": true,
  "scripts": {
    "start": "npm run build && npm run serve",
    "deploy": "npm --no-git-tag-version version patch",
    "build": "tsc",
    "serve": "node ./bin/Quickpaste-Server.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run deploy && git add ."
    }
  },
  "authors": [
    "Alfagun74"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "husky": "^5.0.9",
    "nanoid": "^3.1.20",
    "pretty-bytes": "^5.5.0",
    "tslog": "^3.1.1",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.29"
  }
}
